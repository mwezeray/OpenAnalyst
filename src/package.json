			},
			{
				"command": "oa-code.helpButtonClicked",
				"title": "%command.documentation.title%",
				"icon": "$(question)"
			},
			{
				"command": "oa-code.openInNewTab",
				"title": "%command.openInNewTab.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.explainCode",
				"title": "%command.explainCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.fixCode",
				"title": "%command.fixCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.improveCode",
				"title": "%command.improveCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.addToContext",
				"title": "%command.addToContext.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.newTask",
				"title": "%command.newTask.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.terminalAddToContext",
				"title": "%command.terminal.addToContext.title%",
				"category": "%extension.displayName%"
			},
			{
				"command": "oa-code.terminalFixCommand",
				"title": "%command.terminal.fixCommand.title%",
				"category": "%extension.displayName%"
			},
			{
				"command": "oa-code.terminalExplainCommand",
				"title": "%command.terminal.explainCommand.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.generateTerminalCommand",
				"title": "%command.terminal.generateCommand.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.setCustomStoragePath",
				"title": "%command.setCustomStoragePath.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.focusChatInput",
				"title": "%command.focusInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.acceptInput",
				"title": "%command.acceptInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.profileButtonClicked",
				"title": "%command.profile.title%",
				"icon": "$(account)",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.generateCommitMessage",
				"title": "%command.generateCommitMessage.title%",
				"icon": {
					"light": "assets/icons/oa-light.svg",
					"dark": "assets/icons/oa-dark.svg"
				},
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.ghost.generateSuggestions",
				"title": "%ghost.commands.generateSuggestions%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.ghost.cancelSuggestions",
				"title": "%ghost.commands.cancelSuggestions%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.ghost.applyCurrentSuggestions",
				"title": "%ghost.commands.applyCurrentSuggestion%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.ghost.applyAllSuggestions",
				"title": "%ghost.commands.applyAllSuggestions%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.ghost.promptCodeSuggestion",
				"title": "%ghost.commands.promptCodeSuggestion%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.ghost.goToNextSuggestion",
				"title": "%ghost.commands.goToNextSuggestion%",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.ghost.goToPreviousSuggestion",
				"title": "%ghost.commands.goToPreviousSuggestion%",
				"category": "%configuration.title%"
			}
		],
		"keybindings": [
			{
				"command": "oa-code.focusChatInput",
				"key": "ctrl+shift+a",
				"mac": "cmd+shift+a",
				"when": "true"
			},
			{
				"command": "editor.action.inlineSuggest.commit",
				"key": "tab",
				"when": "inlineSuggestionVisible && editorTextFocus && !editorTabMovesFocus && !inSnippetMode && !suggestWidgetVisible && editorLangId != 'markdown'"
			},
			{
				"command": "oa-code.ghost.applyAllSuggestions",
				"key": "shift+tab",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.hasSuggestions"
			},
			{
				"command": "oa-code.ghost.applyCurrentSuggestions",
				"key": "tab",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.hasSuggestions"
			},
			{
				"command": "oa-code.ghost.cancelRequest",
				"key": "escape",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.isProcessing"
			},
			{
				"command": "oa-code.ghost.cancelSuggestions",
				"key": "escape",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.hasSuggestions"
			},
			{
				"command": "oa-code.ghost.goToNextSuggestion",
				"key": "down",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.hasSuggestions"
			},
			{
				"command": "oa-code.ghost.goToPreviousSuggestion",
				"key": "up",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.hasSuggestions"
			},
			{
				"command": "oa-code.ghost.promptCodeSuggestion",
				"key": "ctrl+i",
				"mac": "cmd+i",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.enableQuickInlineTaskKeybinding && !github.copilot.completions.enabled"
			},
			{
				"command": "oa-code.ghost.generateSuggestions",
				"key": "ctrl+l",
				"mac": "cmd+l",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.enableSmartInlineTaskKeybinding && !github.copilot.completions.enabled"
			},
			{
				"command": "oa-code.ghost.showIncompatibilityExtensionPopup",
				"key": "ctrl+l",
				"mac": "cmd+l",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.enableSmartInlineTaskKeybinding && github.copilot.completions.enabled"
			},
			{
				"command": "oa-code.ghost.showIncompatibilityExtensionPopup",
				"key": "ctrl+i",
				"mac": "cmd+i",
				"when": "editorTextFocus && !editorTabMovesFocus && !inSnippetMode && oacode.ghost.enableQuickInlineTaskKeybinding && github.copilot.completions.enabled"
			},
			{
				"command": "oa-code.generateTerminalCommand",
				"key": "ctrl+shift+g",
				"mac": "cmd+shift+g"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "oa-code.contextMenu",
					"group": "navigation"
				}
			],
			"oa-code.contextMenu": [
				{
					"command": "oa-code.explainCode",
					"group": "1_actions@1"
				},
				{
					"command": "oa-code.fixCode",
					"group": "1_actions@2"
				},
				{
					"command": "oa-code.improveCode",
					"group": "1_actions@3"
				},
				{
					"command": "oa-code.addToContext",
					"group": "1_actions@4"
				}
			],
			"terminal/context": [
				{
					"submenu": "oa-code.terminalMenu",
					"group": "navigation"
				}
			],
			"oa-code.terminalMenu": [
				{
					"command": "oa-code.terminalAddToContext",
					"group": "1_actions@1"
				},
				{
					"command": "oa-code.terminalFixCommand",
					"group": "1_actions@2"
				},
				{
					"command": "oa-code.terminalExplainCommand",
					"group": "1_actions@3"
				}
			],
			"view/title": [
				{
					"command": "oa-code.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == oa-code.SidebarProvider"
				},
				{
					"command": "oa-code.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == oa-code.SidebarProvider"
				},
				{
					"command": "oa-code.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == oa-code.SidebarProvider"
				},
				{
					"command": "oa-code.profileButtonClicked",
					"group": "navigation@4",
					"when": "view == oa-code.SidebarProvider"
				},
				{
					"command": "oa-code.settingsButtonClicked",
					"group": "navigation@5",
					"when": "view == oa-code.SidebarProvider"
				},
				{
					"command": "oa-code.popoutButtonClicked",
					"group": "navigation@6",
					"when": "view == oa-code.SidebarProvider"
				},
				{
					"command": "oa-code.helpButtonClicked",
					"group": "navigation@8",
					"when": "false && view == oa-code.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "oa-code.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == oa-code.TabPanelProvider"
				},
				{
					"command": "oa-code.mcpButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == oa-code.TabPanelProvider"
				},
				{
					"command": "oa-code.historyButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == oa-code.TabPanelProvider"
				},
				{
					"command": "oa-code.popoutButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == oa-code.TabPanelProvider"
				},
				{
					"command": "oa-code.settingsButtonClicked",
					"group": "navigation@5",
					"when": "activeWebviewPanelId == oa-code.TabPanelProvider"
				},
				{
					"command": "oa-code.helpButtonClicked",
					"group": "navigation@6",
					"when": "activeWebviewPanelId == oa-code.TabPanelProvider"
				}
			],
			"scm/input": [
				{
					"command": "oa-code.generateCommitMessage",
					"group": "navigation"
				}
			],
			"scm/title": [
				{
					"command": "oa-code.generateCommitMessage",
					"when": "scmProvider == git",
					"group": "navigation"
				}
			]
		},
		"submenus": [
			{
				"id": "oa-code.contextMenu",
				"label": "%views.contextMenu.label%"
			},
			{
				"id": "oa-code.terminalMenu",
				"label": "%views.terminalMenu.label%"
			}
		],
		"configuration": {
			"title": "%configuration.title%",
			"properties": {
				"oa-code.allowedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"npm test",
						"npm install",
						"tsc",
						"git log",
						"git diff",
						"git show"
					],
					"description": "%commands.allowedCommands.description%"
				},
				"oa-code.deniedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [],
					"description": "%commands.deniedCommands.description%"
				},
				"oa-code.commandExecutionTimeout": {
					"type": "number",
					"default": 0,
					"minimum": 0,
					"maximum": 600,
					"description": "%commands.commandExecutionTimeout.description%"
				},
				"oa-code.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.vendor.description%"
						},
						"family": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.family.description%"
						}
					},
					"description": "%settings.vsCodeLmModelSelector.description%"
				},
				"oa-code.customStoragePath": {
					"type": "string",
					"default": "",
					"description": "%settings.customStoragePath.description%"
				},
				"oa-code.enableCodeActions": {
					"type": "boolean",
					"default": true,
					"description": "%settings.enableCodeActions.description%"
				},
				"oa-code.autoImportSettingsPath": {
					"type": "string",
					"default": "",
					"description": "%settings.autoImportSettingsPath.description%"
				},
				"oa-code.useAgentRules": {
					"type": "boolean",
					"default": true,
					"description": "%settings.useAgentRules.description%"
				}
			}
		},
		"codeActions": [
			{
				"languages": [
					"*"
				],
				"providedCodeActionKinds": [
					"vscode.CodeActionKind.QuickFix"
				]
			}
		]
	},
	"scripts": {
		"lint": "eslint . --ext=ts --max-warnings=0",
		"check-types": "tsc --noEmit",
		"pretest": "turbo run bundle --cwd ..",
		"test": "vitest run",
		"format": "prettier --write .",
		"bundle": "node esbuild.mjs",
		"vscode:prepublish": "pnpm bundle --production",
		"vsix": "mkdirp ../bin && vsce package --no-dependencies --out ../bin",
		"publish:marketplace": "vsce publish --no-dependencies && ovsx publish --no-dependencies",
		"watch:bundle": "pnpm bundle --watch",
		"watch:tsc": "cd .. && tsc --noEmit --watch --project src/tsconfig.json",
		"clean": "rimraf README.md CHANGELOG.md LICENSE dist logs mock .turbo"
	},
