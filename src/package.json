			},
			{
				"command": "oa-code.exportSettings",
				"title": "Export Settings",
				"category": "%configuration.title%"
			},
			{
				"command": "oa-code.promptsButtonClicked",
				"title": "%command.prompts.title%",
				"icon": "$(organization)"
			},
			{
				"command": "oa-code.historyButtonClicked",
