{
	"name": "oa-code",
	"displayName": "%extension.displayName%",
	"description": "%extension.description%",
	"publisher": "oacode",
	"version": "4.79.1",
	"icon": "assets/icons/logo-outline-black.png",
	"galleryBanner": {
		"color": "#FFFFFF",
		"theme": "light"
	},
	"engines": {
		"vscode": "^1.84.0",
		"node": "20.19.2"
	},
	"author": {
		"name": "OpenAnalyst"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Oa-Org/oacode"
	},
	"homepage": "https://oacode.ai",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama",
		"OpenAnalyst",
		"oacode",
		"roo code",
		"roocode"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"walkthroughs": [
			{
				"id": "oaCodeWalkthrough",
				"title": "5 ways OpenAnalyst helps you analyse the data through code",
				"description": "You now have a personal AI Data Analyst assistant.",
				"steps": [
					{
						"id": "welcome",
						"title": "Write code for you",
						"description": "Describe what you want to build. OpenAnalyst will write it from scratch, generate the files, and run them for you.",
						"media": {
							"markdown": "./dist/walkthrough/step1.md"
						}
					},
					{
						"id": "getting-started",
						"title": "Understand your codebase",
						"description": "Need clarity on how something works? Ask OpenAnalyst about any part of your files and get a clear explanation.",
						"media": {
							"markdown": "./dist/walkthrough/step2.md"
						},
						"content": {
							"path": "./dist/walkthrough/step2.md"
						}
					},
					{
						"id": "modes",
						"title": "Get things working again",
						"description": "Stuck on an error? OpenAnalyst can find the issue, fix it, and get your code running.",
						"media": {
							"markdown": "./dist/walkthrough/step3.md"
						}
					},
					{
						"id": "code-actions",
						"title": "Plan your logic",
						"description": "Not sure where to start? Map out your logic and structure before writing a single line of code.",
						"media": {
							"markdown": "./dist/walkthrough/step4.md"
						}
					},
					{
						"id": "advanced-features",
						"title": "And more",
						"description": "Improve your prompt, add context, or create your own custom modes.",
						"media": {
							"markdown": "./dist/walkthrough/step5.md"
						}
					}
				]
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "oa-code-ActivityBar",
					"title": "%views.activitybar.title%",
					"icon": "assets/icons/oa.svg",
					"darkIcon": "assets/icons/oa-dark.svg",
					"when": "isMac || isLinux || isWindows"
					
				}
			]
		},
		"views": {
			"oa-code-ActivityBar": [
				{
					"icon": "assets/icons/oa.svg",
					"type": "webview",
					"id": "oa-code.SidebarProvider",
					"name": "%views.sidebar.name%"
				}
			]
		},
		"commands": [
